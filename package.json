{
  "name": "my-portfolio",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-image-magnifiers": "^1.4.0",
    "react-reveal": "^1.2.2",
    "react-sticky": "^6.0.3",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "load": "node --version && npm install",
    "es:lint": "node_modules/.bin/eslint --ext=js --ext=ts --ext=jsx --ext=tsx .",
    "style:lint": "node_modules/.bin/stylelint \"**/*.style.ts*\"",
    "lint": "npm run es:lint && npm run style:lint",
    "test": "npm run build && concurrently -k \"npm run start\" \"npx cypress open\"",
    "start": "serve build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev": "concurrently -k \"npm run watch\" \"npm run start\"",
    "watch": "cross-env NODE_ENV=development webpack --progress --watch",
    "licenses:ci": "license-report --config license-report.config.json > licenses.csv",
    "test:ci": "npm run build && concurrently -k \"npm run start\" \"npx cypress run\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.12.1",
    "@cypress/react": "^5.10.0",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.19",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-image-magnifiers": "^1.3.1",
    "@types/react-sticky": "^6.0.4",
    "@types/styled-components": "^5.1.12",
    "@types/stylelint": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel": "^6.23.0",
    "clean-webpack-plugin": "*",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.1",
    "cypress-react-unit-test": "^4.17.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "serve": "^12.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.1"
  }
}
